{
    "name": "captenmasin/inertia-seo",
    "description": "This is my package inertia-seo",
    "keywords": [
        "Mason Day",
        "laravel",
        "inertia-seo"
    ],
    "homepage": "https://github.com/captenmasin/inertia-seo",
    "license": "MIT",
    "authors": [
        {
            "name": "Mason Day",
            "email": "daymason@hotmail.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "artesaos/seotools": "^1.3",
        "illuminate/contracts": "^10.0||^11.0",
        "inertiajs/inertia-laravel": "^1.3",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Captenmasin\\InertiaSeo\\": "src/",
            "Captenmasin\\InertiaSeo\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Captenmasin\\InertiaSeo\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-inertia-seo --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Captenmasin\\InertiaSeo\\InertiaSeoServiceProvider"
            ],
            "aliases": {
                "InertiaSeo": "Captenmasin\\InertiaSeo\\Facades\\InertiaSeo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
